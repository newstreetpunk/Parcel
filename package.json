{
  "name": "start-parcel-project-with-tailwindcss",
  "version": "1.0.0",
  "source": "src/index.html",
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "namerSourceFolder": "src",
  "namerContentHash": true,
  "parcelClean": [
    "docs/js",
    "docs/scss",
    "docs/css",
    "*.html",
    "*.js",
    "!build.js"
  ],
  "parcelBuildClean": [
    "docs",
    ".parcel-cache"
  ],
  "scripts": {
    "dev": "parcel --open --dist-dir docs",
    "build": "parcel build --no-source-maps --no-cache --no-optimize --dist-dir docs --public-url ./",
    "prod": "parcel build --no-source-maps --no-cache --dist-dir docs --public-url ./"
  },
  "devDependencies": {
    "@parcel/transformer-sass": "^2.8.3",
    "parcel": "^2.8.3",
    "parcel-reporter-clean-dist": "^1.0.4",
    "postcss": "^8.4.23",
    "posthtml-img-autosize": "^0.1.6",
    "posthtml-modules": "^0.9.1",
    "tailwindcss": "^3.3.1"
  },
  "dependencies": {
    "@pritesh9/parcel-namer-preserve-structure": "^1.2.0"
  }
}
